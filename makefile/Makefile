# program: program.o
# 	gcc program.o program
# program.o: program.c
# 	gcc -c program.c -o program.o
# ==================================================
# program: program.c
# 	gcc -o program program.c
# ==================================================
# qqq: www
# 	cp www qqq
# www:
# 	echo www > www
# ==================================================
CP=cp
qqq: www
	${CP} www qqq
www:
	echo www > www
# ==================================================
# ==================================================
# ==================================================
# ==================================================
